{
  "name": "yktakaha-4-github-io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "upgrade-interactive": "npm-check --update",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start:external": "docusaurus start --host 0.0.0.0",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "generate:pdf": "bash ./pdf/generate_pdf.bash",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "fix": "tsc && textlint --fix src/ && eslint --fix '**/*.{ts,tsx,js}' && prettier --write '**/*.{ts,tsx,js,mdx,md,json,yaml,yml}'",
    "lint": "tsc && textlint src/ && eslint '**/*.{ts,tsx,js}' && prettier -c '**/*.{ts,tsx,js,mdx,md,json,yaml,yml}'",
    "test": "jest --runInBand",
    "test:e2e": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --runInBand --config jest.config.e2e.js",
    "refresh-components-data": "npm run fetch-github-pull-requests && npm run fetch-qiita-items && npm run fetch-zenn-articles && npm run fetch-zenn-topics && npm run fetch-note-contents && npm run build-components-data",
    "build-components-data": "ts-node -P tsconfig.scripts.json src/services/scripts/buildComponentsData.ts",
    "fetch-github-pull-requests": "ts-node -P tsconfig.scripts.json src/services/scripts/fetchGitHubPullRequests.ts",
    "fetch-qiita-items": "ts-node -P tsconfig.scripts.json src/services/scripts/fetchQiitaItems.ts",
    "fetch-zenn-articles": "ts-node -P tsconfig.scripts.json src/services/scripts/fetchZennArticles.ts",
    "fetch-zenn-topics": "ts-node -P tsconfig.scripts.json src/services/scripts/fetchZennTopics.ts",
    "fetch-note-contents": "ts-node -P tsconfig.scripts.json src/services/scripts/fetchNoteContents.ts"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@mdx-js/react": "^3.1.0",
    "@octokit/graphql": "^9.0.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "fs-extra": "^11.2.0",
    "log4js": "^6.9.1",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.39.1",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.4",
    "cross-fetch": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "nock": "^14.0.10",
    "npm-check": "^6.0.1",
    "pdfjs-dist": "^5.4.54",
    "prettier": "^3.3.3",
    "textlint": "^15.2.1",
    "textlint-rule-preset-jtf-style": "^3.0.2",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=22.0"
  }
}
