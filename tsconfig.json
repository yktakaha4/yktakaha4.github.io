{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "target": "esnext",
    "paths": {
      "@/*": ["./src/*"],
      "@site/*": ["./*"]
    },
    "jsx": "react-jsx",
    "types": ["vitest/globals"]
  },
  "ts-node": {
    "esm": true,
    "require": ["tsconfig-paths/register"]
  }
}
